	<queryString>
		<![CDATA[SELECT
proveedor.nombre,
proveedor.contacto,
proveedor.telefono,
proveedor.observacion,
proveedor.correo,
proveedor.estado,
proveedor.direccion,
proveedor.nit,
marca.nombre AS marca,
proveedor.ide
FROM
proveedor
INNER JOIN marca ON marca.idep = proveedor.ide
ORDER BY
proveedor.estado ASC,
proveedor.ide ASC]]>
